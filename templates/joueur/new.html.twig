                            <a href="{{ path('app_joueur_show', {'idjoueur': joueur.idjoueur}) }}">üëÅÔ∏è</a>
                            <a href="{{ path('app_joueur_edit', {'idjoueur': joueur.idjoueur}) }}">‚úèÔ∏è</a>
